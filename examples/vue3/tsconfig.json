{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "types": ["vite/client", "node", "vite-svg-loader", "element-plus/global"]
  },
  "include": ["views/**.ts", "./*.ts", "./*.d.ts", "src/main.ts", "src/router.ts"]
}